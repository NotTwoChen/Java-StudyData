package homeWork;

public class Doctor extends Person implements UserInter {

	@Override
	public void onLine() {
		System.out.println(getWork() + "动手术的时候满头大汗");
	}

	@Override
	public void offLine() {

	}
	
	public void onLine(String username) {
		System.out.println(username + "开始做手术了");
	}
	public void offLine(String username) {
		System.out.println(username + "做完累昏了");
	}

}
