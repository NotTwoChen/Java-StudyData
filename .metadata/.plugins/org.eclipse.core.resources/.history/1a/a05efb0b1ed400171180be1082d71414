package com.lanou3g.main;

public class StringDemo {

	public static void main(String[] args) {
		/**
		 * String类表示字符串
		 * 注意,字符串不是基本数据类型
		 */
		String s1 = "张三";
		String s2 = "张三";
		String s3 = new String("张三");
		String s4 = new String("张三");
		
		
		System.out.println(s1 == s2);
		System.out.println(s3 == s4);
	}

}
