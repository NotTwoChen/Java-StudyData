		System.out.println("请输入选手百米赛跑所好耗费的时间");
		int time = scanner.nextInt();
		System.out.println("请输入选手的性别:1.男/2.女");
		int sex = scanner.nextInt();
		if (time <= 10) {
			System.out.println("该选手进入决赛");
			if (sex == 1) {
				System.out.println("该选手进入男子组");
			}else if(sex == 2){
				System.out.println("该选手进入女子组");
			}else {
				System.out.println("性别选项输入错误,请重新输入");
			}
		}
		main(args);
		
	}
