package homeWork;

public class Cooker implements UserInter {

	@Override
	public void onLine() {

	}

	@Override
	public void offLine() {

	}
	
	public void onLine(String username) {
		System.out.println(username + "开始做饭了");
	}
	public void offLine(String username) {
		System.out.println(username + "做完了可以吃了");
	}

}
